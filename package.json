{
  "name": "cyc-blog",
  "version": "1.0.0",
  "description": "cyc-blog",
  "author": "chengeng",
  "private": true,
  "scripts": {
    "dev": "cross-env NUXT_ENV_BUILD_ENV=development NODE_ENV=development nodemon server/index.js --watch server",
    "build:test": "NUXT_ENV_BUILD_ENV=test nuxt build",
    "build:prod": "NUXT_ENV_BUILD_ENV=production nuxt build",
    "start:test": "cross-env NUXT_ENV_BUILD_ENV=test NODE_ENV=production node server/index.js",
    "start:prod": "cross-env NUXT_ENV_BUILD_ENV=production NODE_ENV=production node server/index.js",
    "pm2:prod": "pm2 start ./server/pm2/prod/pm2.json",
    "pm2:test": "pm2 start ./server/pm2/test/pm2.json",
    "pm2:reload-test": "pm2 reload test-diandian-web",
    "pm2:delete-prod": "pm2 delete prod-diandian-web",
    "pm2:restart-prod": "npm-run-all -s pm2:delete-prod pm2:prod",
    "analyze": "NUXT_ENV_BUILD_ENV=production nuxt build -a",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint-fix": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "test": "jest",
    "storybook": "start-storybook -p 6060",
    "build-storybook": "build-storybook -c .storybook -o .storybook.out",
    "tree": "tree -L 3 -I \"node_modules\"",
    "tdk": "node ./utils/tdkTools.js"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/dayjs": "^1.2.0",
    "@nuxtjs/dotenv": "^1.4.0",
    "@nuxtjs/proxy": "^1.3.3",
    "@nuxtjs/style-resources": "^1.0.0",
    "cookie-universal-nuxt": "^2.1.4",
    "core-js": "2.x",
    "cross-env": "^5.2.0",
    "dayjs": "^1.8.28",
    "diff-match-patch": "^1.0.5",
    "echarts": "^5.3.0",
    "element-ui": "^2.13.2",
    "form-data": "^3.0.0",
    "html-to-md": "^0.3.5",
    "koa": "^2.6.2",
    "koa-bodyparser": "^4.3.0",
    "koa-router": "^8.0.8",
    "lodash": "^4.17.15",
    "lodash-webpack-plugin": "^0.11.5",
    "marked": "^2.1.3",
    "mavon-editor": "^2.9.1",
    "npm-run-all": "^4.1.5",
    "nuxt": "^2.0.0",
    "qs": "^6.9.4",
    "v-charts": "^1.19.0",
    "valine": "^1.4.14",
    "videojs-contrib-hls": "^5.15.0",
    "vue-clipboard2": "^0.3.1",
    "vue-i18n": "^8.18.1",
    "vue-infinite-scroll": "^2.0.2",
    "vue-qr": "^2.2.1",
    "vue-video-player": "^5.0.2"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^2.0.0",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@nuxtjs/stylelint-module": "^3.1.0",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-docs": "^5.3.19",
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/addon-notes": "^5.3.19",
    "@storybook/addon-storysource": "^5.3.19",
    "@storybook/addon-viewport": "^5.3.19",
    "@storybook/vue": "^5.3.18",
    "@vue/test-utils": "^1.0.0-beta.27",
    "autoprefixer": "^9.8.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-lodash": "^3.3.4",
    "babel-preset-vue": "^2.0.2",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^24.1.0",
    "node-sass": "^4.14.1",
    "node-xlsx": "^0.15.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.19.1",
    "sass-loader": "^8.0.2",
    "storybook-readme": "^5.0.8",
    "stylelint": "^10.1.0",
    "vue-jest": "^4.0.0-0"
  }
}
