{
  "apps": [
    {
      "name": "prod-diandian-web",
      "script": "./server/index.js",
      "env": {
        "NODE_ENV": "production",
        "NUXT_ENV_BUILD_ENV": "production"
      },

      "log_date_format": "YYYY-MM-DD HH:mm Z",
      "error_file": "~/.pm2/logs/prod-diandian-web-error.log",
      "out_file": "~/.pm2/logs/prod-diandian-web-out.log",
      "instances": 0,
      "min_uptime": "200s",
      "max_restarts": 0,
      "max_memory_restart": "500M",
      "watch": false,
      "merge_logs": true,
      "exec_interpreter": "node",
      "exec_mode": "cluster",
      "vizion": false
    }
  ]
}
